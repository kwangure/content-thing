{
	"name": "@content-thing/remark-yaml-parse",
	"version": "0.0.1",
	"scripts": {
		"build": "wireit",
		"pre-commit": "wireit",
		"publint": "wireit",
		"release": "wireit",
		"test": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --build",
			"files": [
				"src/**/*.{js,ts}",
				"tests/**/*.{js,ts}",
				"tsconfig.json",
				"../../config/tsconfig.base.json"
			],
			"output": [
				"dist"
			]
		},
		"publint": {
			"command": "publint",
			"dependencies": [
				"build"
			],
			"files": [
				"src/**",
				"dist/**",
				"package.json"
			],
			"output": []
		},
		"pre-commit": {
			"dependencies": [
				"build",
				"publint",
				"test"
			]
		},
		"release": {
			"dependencies": [
				"build",
				"publint",
				"test"
			]
		},
		"test": {
			"command": "node --test --test-reporter spec",
			"files": [
				"src/**/*.{js,ts}",
				"tests/**/*.{js,ts}"
			],
			"output": []
		}
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.spec.*"
	],
	"dependencies": {
		"js-yaml": "^4.1.0",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.5",
		"@types/mdast": "^4.0.1",
		"@types/node": "^20.4.10",
		"publint": "^0.2.2",
		"remark-frontmatter": "^5.0.0",
		"remark-parse": "^11.0.0",
		"tsx": "^4.7.0",
		"typescript": "^5.1.6",
		"unified": "^11.0.3"
	},
	"license": "MIT",
	"type": "module"
}
